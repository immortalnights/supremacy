{
  "name": "supremacy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev-client": "cd client && npm start",
    "dev-sim-client": "cd simulation && tsc --watch --outDir \"./../client/public/supremacy\"",
    "dev-sim-types-client": "cd simulation && npx tsc --declaration  --outDir \"../client/src/simulation\" --emitDeclarationOnly \"./types.ts\" --watch",
    "dev-sim-types-server": "cd simulation && npx tsc --declaration  --outDir \"../server/simulation\" --emitDeclarationOnly \"./types.ts\" --watch",
    "NOdev-sim-server": "cd simulation && tsc --watch --outDir \"./../server/supremacy\"",
    "dev-server": "cd server & npx nodemon ./index.ts",
    "start-dev2": "concurrently \"npm:dev-*\"",
    "start-dev": "concurrently --prefix \"[{name}]\" --names \"   SIM,SIM_TYP,CLIENT\" -c \"bgBlue.bold,bgBlue,bgGreen.bold\" \"npm run build-sim\" \"npm run build-sim-types\" \"npm run dev-client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^7.0.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "express": "^4.17.2"
  }
}
