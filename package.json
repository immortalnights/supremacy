  "main": "index.js",
  "scripts": {
    "dev-client": "cd client && npm start",
    "build-sim": "cd simulation && tsc --watch",
    "build-sim-types": "cd simulation && npx tsc --declaration  --outDir \"../client/src/simulation\" --emitDeclarationOnly \"./types.ts\" --watch",
    "dev": "concurrently --prefix \"[{name}]\" --names \"   SIM,SIM_TYP,CLIENT\" -c \"bgBlue.bold,bgBlue,bgGreen.bold\" \"npm run build-sim\" \"npm run build-sim-types\" \"npm run dev-client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
