{
  "name": "supremacy-server",
  "version": "0.2.0",
  "author": "Andrew K",
  "license": "MIT",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "dev-client": "cd ../client && npm start",
    "dev-server-types-client": "npx tsc \"types.ts\" --outdir \"../client/src\" --declaration --emitDeclarationOnly --watch",
    "dev-sim-types-client": "npx tsc \"simulation/types.ts\" --outdir \"../client/src/simulation\" --declaration --emitDeclarationOnly --watch",
    "dev-server": "npx nodemon ./index.ts --config ./nodemon.json",
    "electron": "electron .",
    "build-server": "tsx .",
    "start-dev": "concurrently \"npm:dev-*\"",
    "test-server": "jest"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "socket.io": "^4.7.2"
  }
}
